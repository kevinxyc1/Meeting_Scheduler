<button onclick="output()">Submit</button>

<p id="demo"></p>
<p id="demoMonday1"></p>
<p id="demoMonday2"></p>
<p id="demoTuesday1"></p>
<p id="demoTuesday2"></p>
<p id="demoWednesday1"></p>
<p id="demoWednesday2"></p>
<p id="demoThursday1"></p>
<p id="demoThursday2"></p>
<p id="demoFriday1"></p>
<p id="demoFriday2"></p>
<p id="demoSaturday1"></p>
<p id="demoSaturday2"></p>
<p id="demoSunday1"></p>
<p id="demoSunday2"></p>

<script>
var valMonday = "";
var valTuesday = "";
var valWednesday = "";
var valThursday = "";
var valFriday = "";
var valSaturday = "";
var valSunday = "";

function output(){

document.getElementById("demo").innerHTML = "The two of you are both free on the following days:";

var a1 = document.querySelectorAll("#user1time option");
var a2 = document.querySelectorAll("#user2time option");
var b1 = document.querySelectorAll("#user3time option");
var b2 = document.querySelectorAll("#user4time option");
var c1 = document.querySelectorAll("#user5time option");
var c2 = document.querySelectorAll("#user6time option");
var d1 = document.querySelectorAll("#user7time option");
var d2 = document.querySelectorAll("#user8time option");
var e1 = document.querySelectorAll("#user9time option");
var e2 = document.querySelectorAll("#user10time option");
var f1 = document.querySelectorAll("#user11time option");
var f2 = document.querySelectorAll("#user12time option");
var g1 = document.querySelectorAll("#user13time option");
var g2 = document.querySelectorAll("#user14time option");

for (var h = 0; h < 12; h++){
	if ((a1[h].selected && a2[h].selected) && (a1[h].value == a2[h].value)){
		valMonday += a1[h].value;
		valMonday += ' ';
	}
document.getElementById("demoMonday1").innerHTML = "Monday:";
document.getElementById("demoMonday2").innerHTML = valMonday;
}

for (var i = 0; i < 12; i++){
	if ((b1[i].selected && b2[i].selected) && (b1[i].value == b2[i].value)){
		valTuesday += b1[i].value;
		valTuesday += ' ';
	}
document.getElementById("demoTuesday1").innerHTML = "Tuesday:";
document.getElementById("demoTuesday2").innerHTML = valTuesday;
}

for (var j = 0; j < 12; j++){
	if ((c1[j].selected && c2[j].selected) && (c1[j].value == c2[j].value)){
		valWednesday += c1[j].value;
		valWednesday += ' ';
	}
document.getElementById("demoWednesday1").innerHTML = "Wednesday:";
document.getElementById("demoWednesday2").innerHTML = valWednesday;
}

for (var k = 0; k < 12; k++){
	if ((d1[k].selected && d2[k].selected) && (d1[k].value == d2[k].value)){
		valThursday += d1[k].value;
		valThursday += ' ';
	}
document.getElementById("demoThursday1").innerHTML = "Thursday:";
document.getElementById("demoThursday2").innerHTML = valThursday;
}

for (var l = 0; l < 12; l++){
	if ((e1[l].selected && e2[l].selected) && (e1[l].value == e2[l].value)){
		valFriday += e1[l].value;
		valFriday += ' ';
	}
document.getElementById("demoFriday1").innerHTML = "Friday:";
document.getElementById("demoFriday2").innerHTML = valFriday;
}

for (var m = 0; m < 12; m++){
	if ((f1[m].selected && f2[m].selected) && (f1[m].value == f2[m].value)){
		valSaturday += f1[m].value;
		valSaturday += ' ';
	}
document.getElementById("demoSaturday1").innerHTML = "Saturday:";
document.getElementById("demoSaturday2").innerHTML = valSaturday;
}

for (var n = 0; n < 12; n++){
	if ((g1[n].selected && g2[n].selected) && (g1[n].value == g2[n].value)){
		valSunday += g1[n].value;
		valSunday += ' ';
	}
document.getElementById("demoSunday1").innerHTML = "Sunday:";
document.getElementById("demoSunday2").innerHTML = valSunday;
}
}
</script>

